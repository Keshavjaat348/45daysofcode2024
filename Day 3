#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int B1, B2, B3;
        cin >> B1 >> B2 >> B3;
        
        int emptyCount = 0;
        if (B1 == 0) emptyCount++;
        if (B2 == 0) emptyCount++;
        if (B3 == 0) emptyCount++;
        
        if (emptyCount >= 2) {
            cout << "Water filling time" << endl;
        } else {
            cout << "Not now" << endl;
        }
    }
    return 0;
}
